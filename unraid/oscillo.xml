<?xml version="1.0"?>
<Container version="2">
  <Name>Oscillo</Name>
  <Repository>ghcr.io/zachyzissou/interactive-music-3d:latest</Repository>
  <Registry>https://ghcr.io/</Registry>
  <Network>bridge</Network>
  <MyIP></MyIP>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://github.com/zachyzissou/INTERACTIVE-MUSIC-3D/issues</Support>
  <Project>https://github.com/zachyzissou/INTERACTIVE-MUSIC-3D</Project>
